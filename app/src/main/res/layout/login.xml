				<View   android:id="@+id/ellipse_13"
					android:background="@drawable/ellipse_13_shape"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="158dp"
					android:layout_height="145dp" />

				<View   android:id="@+id/ellipse_12"
					android:background="@drawable/ellipse_12_shape"
					 android:layout_marginStart="16dp"
					android:layout_marginTop="99dp"
					android:layout_width="71dp"
					android:layout_height="71dp" />

			</RelativeLayout>


			<RelativeLayout android:id="@+id/group_17"
				android:layout_marginTop="175dp"
				android:layout_marginStart="-10dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<View   android:id="@+id/_bg__group_17_ek1"
					android:background="@drawable/_bg__group_17_ek1_shape"
					 android:layout_marginStart="-314dp"
					android:layout_marginTop="-175dp"
					android:layout_width="109dp"
					android:layout_height="122dp" />

			</RelativeLayout>

			<View
				android:id="@+id/ellipse_14"
				android:layout_width="109dp"
				android:layout_height="104dp"
				android:layout_above="@+id/group_19"
				android:layout_alignEnd="@+id/segera_lihat_resep_makananmu_hari_ini__let_s_go_healthly"
				android:layout_marginTop="0dp"
				android:layout_marginEnd="-120dp"
				android:layout_marginBottom="-390dp"
				android:background="@drawable/ellipse_14_shape" />

			<View
				android:id="@+id/ellipse_15"
				android:layout_width="42dp"
				android:layout_height="42dp"
				android:layout_alignEnd="@+id/segera_lihat_resep_makananmu_hari_ini__let_s_go_healthly"
				android:layout_alignBottom="@+id/enter_your_account"
				android:layout_marginStart="180dp"
				android:layout_marginEnd="-70dp"
				android:layout_marginBottom="25dp"
				android:layout_toEndOf="@+id/enter_your_account"
				android:background="@drawable/ellipse_15_shape" />

		</RelativeLayout>

	</ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>