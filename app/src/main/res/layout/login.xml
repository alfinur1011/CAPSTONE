				android:layout_height="51.45dp"
				android:layout_marginStart="34dp"
				android:layout_marginTop="122dp"
				android:contentDescription="food_health_high_resolution_logo_transparent_1"
				android:src="@drawable/food_health_high_resolution_logo_transparent_1" />

			<RelativeLayout android:id="@+id/group_18"
				android:layout_marginTop="-27dp"
